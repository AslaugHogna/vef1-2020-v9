{
  "name": "vef1-2020-v9",
  "version": "1.0.0",
  "description": "Verkefni 9 sækir gögn um jarðskjálfta frá USGS og birtir á korti með [Leaflet](https://leafletjs.com/).",
  "main": "index.js",
  "scripts": {
    "watch-sass": "node-sass styles.scss styles.css && node-sass styles.scss styles.css -w",
    "sass": "node-sass styles.scss styles.css",
    "browser-sync": "browser-sync start --server --files index.html styles.css scripts.js",
    "dev": "concurrently --kill-others \"npm run watch-sass\" \"npm run browser-sync\"",
    "lint": "stylelint **/*.scss --syntax scss",
    "test": "eslint *.js **/*.js",
    "build": "rollup -c",
    "rollup-watch": "rollup -c -w",
    "babel": "babel src --out-dir dist",
    "babel-watch": "babel src --out-dir dist --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AslaugHogna/vef1-2020-v9.git"
  },
  "author": "Áslaug Högnadóttir",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/AslaugHogna/vef1-2020-v9/issues"
  },
  "homepage": "https://github.com/AslaugHogna/vef1-2020-v9#readme",
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@rollup/plugin-commonjs": "^16.0.0",
    "@rollup/plugin-node-resolve": "^10.0.0",
    "concurrently": "^5.3.0",
    "eslint": "^7.12.1",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.1",
    "rollup": "^2.33.1",
    "rollup-plugin-babel": "^4.4.0"
  },
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "date-fns": "^2.16.1",
    "leaflet": "^1.7.1",
    "node-sass": "^5.0.0"
  }
}
